{
    "name": "atlas/orm",
    "type": "library",
    "description": "A data mapper ORM for your persistence model (not your domain model).",
    "keywords": [ "sql", "data", "mapper", "mapping", "orm" ],
    "homepage": "http://github.com/atlasphp/Atlas.Orm",
    "license": "MIT",
    "authors": [
        {
            "name": "Atlas Contributors",
            "homepage": "https://github.com/atlasphp/Atlas.Orm/contributors"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "aura/sql": "~2.0",
        "aura/sqlquery": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Atlas\\Orm\\": "src/"
        }
    },
    "require-dev": {
        "atlas/cli": "~1.0",
        "pds/skeleton": "~1.0",
        "phpunit/phpunit": "~6.0",
        "producer/githooks": "0.x@dev"
    },
    "autoload-dev": {
        "psr-4": {
            "Atlas\\Orm\\": "tests/"
        }
    },
    "suggest": {
        "atlas/cli": "A command-line tool to build skeleton classes."
    }
}
